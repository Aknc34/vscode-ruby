{
	"comments": {
		"lineComment": "#",
		"blockComment": ["=begin", "=end"]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["begin","end"],
		["def","end"],
		["if","end"],
		["case","end"],
		["unless","end"],
		["do","end"],
		["class","end"],
		["module","end"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["`", "`"],
		{ "open": "'", "close": "'", "notIn": ["string", "comment"] },
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "`"]
	]
}
